{
  "targets": {
    "neuromarket": {
      "outFile": "build/NeuroMarket.wasm",
      "textFile": "build/NeuroMarket.wat"
    }
  },
  "options": {
    "sourceMap": false,
    "optimizeLevel": 3,
    "shrinkLevel": 1,
    "enable": [
      "sign-extension",
      "mutable-globals",
      "nontrapping-f2i",
      "bulk-memory",
      "simd",
      "reference-types",
      "multi-value"
    ],
    "runtime": "stub",
    "initialMemory": 1,
    "memoryBase": 0,
    "exportStart": "start",
    "transform": ["@btc-vision/opnet-transform"]
  }
}
